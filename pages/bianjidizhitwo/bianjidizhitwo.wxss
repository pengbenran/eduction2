/* pages/receiving/receiving.wxss */
@import "../xinzengdizhi/xinzengdizhi.wxss";
.receiving{
  height: 100%;
  background: #FFF;
  display: flex;
  flex-direction: column;
}
.lianxi{
  height: 70rpx;
  display: flex;
  justify-content:  flex-end;
  align-items: center;
  border-top: 1rpx solid #eee;
  padding: 20rpx;
}
.Contacts{
  font-size: 15px;
  height: 40rpx;
  width: 20%;
}
.con-input{
  font-size: 14px;
  color: #b3b3b3;
  width: 80%
}
.iconed{
  height: 28rpx;
  width: 16rpx;
}
.detailed{
  height: 150rpx;
  display: flex;
  border-top: 1rpx solid #eee;
  padding: 20rpx;
}
.address{
  font-size: 15px;
  height: 40rpx;
  width: 20%;
}
.add-input{
  font-size: 15px;
  width: 80%;
 color:#b3b3b3;
 height: 100%;
}

/*按钮  */
.cellbtn{
  width: 100%;
  height:100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 40rpx;
}
.com-input01{
  height:90rpx;
  line-height: 90rpx;
  width:85%;
  color: #fff;
  border-radius: 45rpx;
  background: #26b4fe;
  text-align: center;
}

.delete{
  border:2rpx solid  #ff373a;
  height:90rpx;
  line-height: 90rpx;
  width:85%;
  color: #fd4344;
  border-radius: 45rpx;
  background: #f5f5f5;
  text-align: center;
}
.cell{
  border: 2rpx solid  #ababab;
  height:90rpx;
  line-height: 90rpx;
  width:85%;
  color: #ababab;
  border-radius: 45rpx;
  background: #f5f5f5;
  text-align: center;
}