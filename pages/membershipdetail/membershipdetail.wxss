/* pages/membershipdetail/membershipdetail.wxss */
image{
  width: 100%;
  height: 100%;
  display: block;
}
.membershipForm{
  width: 95%;
  margin: 10rpx auto;
  background: #fff;
 padding-top:20rpx;
 padding-bottom: 40rpx;
 box-sizing: border-box; 
}
.membershipIcon{
  width: 71rpx;
  height: 65rpx;
  overflow: hidden;
  margin: 0 auto;
}
.membershiptitle{
  width: 100%;
  text-align: center;
  color: #F5E698;
  margin-top: 10rpx;
  margin-bottom: 40rpx;
}
.membershipFormList{
  height: 100rpx;
  width: 80%;
  margin: 0 auto;
  border-bottom: 1rpx solid #C5C5C5;
}
.membershipFormList input{
  width: 100%;
  height: 100%;
  text-align: center;
}
.formBtn{
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background: #F5E698;
  color: #fff;
  margin: 0 auto;
  border-radius: 200rpx;
  margin-top: 40rpx;
}